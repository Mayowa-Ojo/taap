{
  "name": "Hippocampus",
  "version": "1.0",
  "description": "Be deliberate about where you spend your time on the internet",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage", "activeTab", "tabs"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
