{
  "name": "Taap",
  "version": "1.0.0",
  "description": "Taap is a utility-driven chrome extension. Think of it as a swiss army knife of handy widgets.",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage"],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}
